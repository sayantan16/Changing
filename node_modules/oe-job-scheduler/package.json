  "engines": {
    "node": ">=6"
  },
  "main": "index.js",
  "scripts": {
    "pretest": "npm install --no-optional",
    "test": "mocha test/test.js",
    "lint": "eslint .",
    "fix-lint": "eslint --fix .",
    "grunt-cover": "grunt test-with-coverage"
  },
  "dependencies": {
    "node-schedule": "1.3.0",
    "oe-master-job-executor": "^2.0.0"
  },
  "devDependencies": {
    "babel-eslint": "7.2.3",
    "chai": "3.4.1",
    "chai-things": "0.2.0",
    "chalk": "1.1.1",
    "eslint": "4.10.0",
    "grunt": "1.0.4",
    "grunt-cli": "1.3.2",
    "grunt-contrib-clean": "2.0.0",
    "grunt-mocha-istanbul": "5.0.2",
    "istanbul": "0.4.5",
    "mocha": "5.2.0",
    "oe-cloud": "^2.0.0",
    "oe-connector-mongodb": "^2.0.0",
    "oe-connector-oracle": "^2.0.0",
    "oe-connector-postgresql": "^2.0.0",
    "superagent-defaults": "0.1.14",
    "supertest": "3.4.2"
  },
  "repository": {
    "type": "git",
